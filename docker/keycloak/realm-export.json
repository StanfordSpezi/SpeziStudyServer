{
  "realm": "spezi-study",
  "enabled": true,
  "ssoSessionIdleTimeout": 86400,
  "ssoSessionMaxLifespan": 86400,
  "accessTokenLifespan": 86400,
  "accessTokenLifespanForImplicitFlow": 900,
  "registrationAllowed": true,
  "registrationEmailAsUsername": false,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "groups": [
    {
      "name": "Stanford Biodesign Digital Health",
      "subGroups": [
        {
          "name": "admin",
          "subGroups": []
        },
        {
          "name": "researcher",
          "subGroups": []
        }
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "spezistudyplatform-authorized-users",
        "description": "Users authorized to access the Spezi Study Platform",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "clients": [
    {
      "clientId": "spezi-study-server",
      "name": "Spezi Study Server",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "change-me-in-production",
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "publicClient": false,
      "protocol": "openid-connect",
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true,
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "email",
        "groups"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "clientScopes": [
    {
      "name": "groups",
      "description": "Groups membership with full path",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true"
      },
      "protocolMappers": [
        {
          "name": "groups",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "consentRequired": false,
          "config": {
            "full.path": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "groups",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles",
            "userinfo.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "service-account-spezi-study-server",
      "enabled": true,
      "serviceAccountClientId": "spezi-study-server",
      "clientRoles": {
        "realm-management": [
          "query-groups",
          "view-users"
        ]
      }
    },
    {
      "username": "leland",
      "email": "leland@example.com",
      "emailVerified": true,
      "enabled": true,
      "firstName": "Leland",
      "lastName": "Stanford",
      "credentials": [
        {
          "type": "password",
          "value": "password123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "spezistudyplatform-authorized-users"
      ],
      "groups": [
        "/Stanford Biodesign Digital Health/admin"
      ]
    },
    {
      "username": "jane",
      "email": "jane@example.com",
      "emailVerified": true,
      "enabled": true,
      "firstName": "Jane",
      "lastName": "Stanford",
      "credentials": [
        {
          "type": "password",
          "value": "password123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "spezistudyplatform-authorized-users"
      ],
      "groups": [
        "/Stanford Biodesign Digital Health/researcher"
      ]
    }
  ]
}
